<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="helpdesk_ticket_view_form_show_lost_phone_number" model="ir.ui.view">
        <field name="name">Helpdesk Form: Show Lost Phone Number</field>
        <field name="model">helpdesk.ticket</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_email']" position="before">
                <field name="phone_lost" widget="phone"/>
            </xpath>
        </field>
    </record>
</odoo>